<h1>Items#edit</h1>
<p>Find me in app/views/items/edit.html.erb</p>
<%= form_with model: @item do |form| %>
    <div>
      <%= form.label :name %><br>
      <%= form.text_field :name %>
      
    </div>
  
    <div>
      <%= form.label :quantity %><br>
      <%= form.text_area :quantity %><br>
      
    </div>
  
    <div>
      <%= form.submit %>
    </div>
  <% end %>
